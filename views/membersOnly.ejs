<%- include('templates/header', { 
    title: 'Members Area', 
    cssFile: '' 
}) %>

<div class="d-flex flex-column min-vh-100">

   
    <main class="flex-grow-1 container py-5">
        <h2 class="text-center mb-5">Members Only</h2>

        <div class="row">
            <% images.forEach(image => { %>
                <div class="col-12 col-md-4 mb-4 d-flex justify-content-center">
                    <img src="/images/<%= image %>" class="img-fluid rounded shadow" style="max-height: 250px;" alt="Random Image">
                </div>
            <% }) %>
        </div>
        <div class="text-center">
            <a href="/logout" class="btn btn-danger btn-lg">Logout</a>
        </div>
    </main>
    <%- include('templates/footer', { scripts: [] }) %>
</div>
